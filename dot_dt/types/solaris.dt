		( \
			WHOSE=`file "%(File)Arg_1%" | sed -e "s/.*from .//" -e "s/.\$//"`; \
			WHICH=`which \$WHOSE`; \
			echo "\\\$c" | adb \$WHICH "%(File)Arg_1%"; \
		)| \${PAGER:-more}'
}

