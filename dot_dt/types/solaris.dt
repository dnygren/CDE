ACTION OpenAsCommand
{
	TYPE		COMMAND
	WINDOW_TYPE	NO_STDIO
	EXEC_STRING	ksh -c 'cmd="`head -1 \\"%Arg_1%\\"`"; \
			path="`whence -p \\"\$cmd\\"`"; \
			if [ -f "\$path" ]; then \
				exec dtaction Open "\$path"; \
			else \
				exec dterror.ds "Command: \$cmd" \
					"Not Found" OK; \
			fi'
}

