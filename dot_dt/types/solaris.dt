ACTION Crypt
{
	LABEL		Encryption...
	ICON		Dtkey
	TYPE		MAP
	MAP_ACTION	EnCrypt
	DESCRIPTION	The Encryption... (Crypt) action Encrypts and Decrypts files.
}
ACTION Crypt
{
	ARG_TYPE	CRYPTED
	TYPE		MAP
	MAP_ACTION	DeCrypt
}

ACTION EnCrypt
{
	LABEL		EnCrypt...
	ICON		Dtkey
	TYPE		COMMAND
	WINDOW_TYPE	NO_STDIO
	EXEC_STRING	ksh -c '\
        print "uudecode -p <<\\"endCrypt\\" | crypt \\"\\\$1\\"" \
            >| /tmp/Crypt\$\$; \
        crypt "%"Encryption key:"%" \
            < "%Arg_1"File to crypt:"%" >| /tmp/Crypt\$\$.tmp; \
        if [ $? -eq 0 ]; then \
           uuencode Crypted < /tmp/Crypt\$\$.tmp >> /tmp/Crypt\$\$; \
           if [ $? -eq 0 ]; then \
              cp /tmp/Crypt\$\$ "%Arg_1%"; \
           else \
              dtaction DttermErrorlog; \
           fi; \
        else \
           dtaction DttermErrorlog; \
        fi; \
        rm -f /tmp/Crypt\$\$ /tmp/Crypt\$\$.tmp'
	DESCRIPTION	The EnCrypt... (EnCrypt) action encrypts and uuencodes a file.
}

ACTION DeCrypt
{
	LABEL		DeCrypt...
	ICON		Dtkey
	TYPE		COMMAND
	WINDOW_TYPE	NO_STDIO
	EXEC_STRING	ksh -c '\
		sh "%Arg_1"File to decrypt:"%" \
			"%"Decryption key:"%" \
			>> /tmp/Crypt\$\$; \
        if [ $? -eq 0 ]; then \
            cp /tmp/Crypt\$\$ "%Arg_1%"; \
        else \
            dtaction DttermErrorlog; \
        fi; \
        rm -f /tmp/Crypt\$\$'
	DESCRIPTION	The DeCrypt... (DeCrypt) action uudecodes and decrypts a file.
}

ACTION Date
{
	LABEL		Date
	ICON		Dtclock
	ARG_COUNT	0
	TYPE		COMMAND
	WINDOW_TYPE	NO_STDIO
	EXEC_STRING	sh -c '\
		time="%"Time ([mmdd]HHMM):"%"; \
		case "`id`" in \
			uid=0\\(root*) \
				exec date \$time;; \
			*) \
				dtaction -user root DateDo \$time;; \
		esac
}

