ACTION DbxStack
{
	LABEL		Show Stack Using dbx
	ARG_TYPE	CORE
	ARG_COUNT	1
        TYPE            COMMAND
        WINDOW_TYPE     PERM_TERMINAL
        EXEC_STRING     sh -c ' \
		( \
			WHOSE=`file "%(File)Arg_1%" | sed -e "s/.*from .//" -e "s/.\$//"`; \
			WHICH=`which \$WHOSE`; \
			echo "dbxenv stack_verbose off; where; quit" | dbx \$WHICH "%(File)Arg_1%"; \
		)| \${PAGER:-more}'
}

ACTION AdbStack
{
	LABEL		Show Stack Using adb
	ARG_TYPE	CORE
	ARG_COUNT	1
        TYPE            COMMAND
        WINDOW_TYPE     PERM_TERMINAL
